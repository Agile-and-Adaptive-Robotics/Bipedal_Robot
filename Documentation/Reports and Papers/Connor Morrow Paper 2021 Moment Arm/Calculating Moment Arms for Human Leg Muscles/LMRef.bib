	number = {1},
	urldate = {2018-10-25},
	journal = {The International Journal of Robotics Research},
	author = {Van der Noot, Nicolas and Ijspeert, Auke Jan and Ronsse, Renaud},
	month = jan,
	year = {2018},
	pages = {168--196},
	annote = {Category:


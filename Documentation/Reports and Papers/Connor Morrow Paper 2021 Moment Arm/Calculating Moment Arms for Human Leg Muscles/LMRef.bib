
@incollection{hunt_neuromechanical_2014,
	address = {Milan, Italy},
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {Neuromechanical {Simulation} of an {Inter}-leg {Controller} for {Tetrapod} {Coordination}},
	copyright = {©2014 Springer International Publishing Switzerland},
	isbn = {978-3-319-09434-2 978-3-319-09435-9},
	url = {http://link.springer.com/chapter/10.1007/978-3-319-09435-9_13},
	abstract = {A biologically inspired control system has been developed for coordinating a tetrapod walking gait in the sagittal plane. The controller is built with biologically based neurons and synapses, and connections are based on data from literature where available. It is applied to a simplified, planar biomechanical model of a rat with 14 joints with an antagonistic pair of Hill muscle models per joint. The controller generates tension in the muscles through activation of simulated motoneurons. Though significant portions of the controller are based on cat research, this model is capable of reproducing hind leg behavior observed in walking rats. Additionally, the applied inter-leg coordination pathways between fore and hind legs are capable of creating and maintaining coordination in this rat model. Ablation tests of the different connections involved in coordination indicate the role of each connection in providing coordination with low variability.},
	language = {en},
	number = {8608},
	urldate = {2016-11-07},
	booktitle = {Biomimetic and {Biohybrid} {Systems}},
	publisher = {Springer International Publishing},
	author = {Hunt, Alexander and Schmidt, Manuela and Fischer, Martin and Quinn, Roger D.},
	editor = {Duff, Armin and Lepora, Nathan F. and Mura, Anna and Prescott, Tony J. and Verschure, Paul F. M. J.},
	month = jul,
	year = {2014},
	doi = {10.1007/978-3-319-09435-9_13},
	keywords = {Artificial Intelligence (incl. Robotics), Central pattern generator, Computer Imaging, Vision, Pattern Recognition and Graphics, Control, Robotics, Mechatronics, Data Mining and Knowledge Discovery, Inter-leg Coordination, Mammal, Neural controller, Rat, Theory of Computation},
	pages = {142--153},
	annote = {Category:

In silico (computer simulation) evaluation of an Animatlab controller to coordinate sagittal plane walking gait in a 14 DoF rat model.

Context:

Legged robots locomotion not as good as animals.
Neurobiology theory: hierarchies in the CNS sub-divide complex tasks into subtasks. Brain at the top, motoneurons at the bottom, and each level relies on level-appropriate sensory information to act on the levels below.
Neurological measurement experiments suggest hypothetical circuits.
Non-detailed evidence of CPGs in mammalian systems has resulted in manymodeling theories.
This paper expands upon a state controller for tetrapod hind leg stepping by implementing it in a neurologically based CPG controller.

similar to the one produced by Szczecinski [reference 10].



Correctness:

Generally correct and not a bad match. Hard to quantify error.
Tests done without fully supporting body weight (although this is similar to how tests are done on in vivo animals).
Brains and neural circuitry are very complex, with tens of thousands of connections, highly nonlinear, and involve many parallel processes. Occam's razor does not always hold in these circumstances.

Contributions:

Modeling theories can be tested in Animatlab
In silico ablation tests can be performed.

This can reduce the or eliminate the need to perform these experiments on living animals.
Models can give insight into what/where we should look for future experiments.



Summary:

Biologically inspired controller, based on theoretical neurological circuits, for rat model is simulated. Produces a decent gait match between output and actual animal models.
},
	file = {Hunt et al. - 2014 - Neuromechanical Simulation of an Inter-leg Control.pdf:C\:\\Users\\Connor\\Zotero\\storage\\YTWALTLA\\Hunt et al. - 2014 - Neuromechanical Simulation of an Inter-leg Control.pdf:application/pdf;Snapshot:C\:\\Users\\Connor\\Zotero\\storage\\5ZFE8GZD\\10.html:text/html}
}

@article{hunt_biologically_2015,
	title = {A biologically based neural system coordinates the joints and legs of a tetrapod},
	volume = {10},
	issn = {1748-3190},
	url = {http://stacks.iop.org/1748-3190/10/i=5/a=055004},
	doi = {10.1088/1748-3190/10/5/055004},
	abstract = {A biologically inspired neural control system has been developed that coordinates a tetrapod trotting gait in the sagittal plane. The developed neuromechanical system is used to explore properties of connections in inter-leg and intra-leg coordination. The neural controller is built with biologically based neurons and synapses, and connections are based on data from literature where available. It is applied to a planar biomechanical model of a rat with 14 joints, each actuated by a pair of antagonistic Hill muscle models. The controller generates tension in the muscles through activation of simulated motoneurons. The hind leg and inter-leg control networks are based on pathways discovered in cat research tuned to the kinematic motions of a rat. The foreleg network was developed by extrapolating analogous pathways from the hind legs. The formulated intra-leg and inter-leg networks properly coordinate the joints and produce motions similar to those of a walking rat. Changing the strength of a single inter-leg connection is sufficient to account for differences in phase timing in different trotting rats.},
	language = {en},
	number = {5},
	urldate = {2016-11-07},
	journal = {Bioinspiration \& Biomimetics},
	author = {Hunt, Alexander and Schmidt, Manuela and Fischer, Martin and Quinn, Roger},
	year = {2015},
	pages = {055004},
	annote = {Category:

In silico CPG based neural controller for a planar biomechanical 14 DoF rat model.

 
Context:

Further development of Hunt's Living Machines 2014 paper.
Each joint activated by antagonistic set of muscles using the Hill model.
Added connected circuit from last paper.

Correctness:

Good results.

Contributions:

This is a great method to coordinate tetrapod walking without weight feedback.
Shows that CPG information from cat experiments can be extrapolated to rat models.
Shows that foreleg network is analogous to hind legs.

Summary:

Biologically based controller using hypothesized neuron pathways designed in Animatlab and simulated. Results show this to be a good controller for the system. Need an artificial muscle similar to real muscle for a physical test.
},
	file = {Hunt et al. - 2015 - A biologically based neural system coordinates the.pdf:C\:\\Users\\Connor\\Zotero\\storage\\DHU6ZG88\\Hunt et al. - 2015 - A biologically based neural system coordinates the.pdf:application/pdf}
}

